{
  "name": "iqorbit",
  "version": "1.0.0",
  "description": "IQOrbit is an Aptitude API which provide you a reach quality of aptitude questions",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && pnpm start",
    "clear": "rmdir /s /q dist",
    "clear:file": "del data.json",
    "clear:all": "rmdir /s /q dist",
    "start": "node dist/main.js",
    "dev": "tsx watch src/main.ts"
  },
  "keywords": [
    "Aptitude",
    "Aptitude API",
    "QnA"
  ],
  "author": "ankan",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.2.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.5",
    "mongoose": "^8.5.2",
    "sync-fetch": "^0.5.2"
  }
}